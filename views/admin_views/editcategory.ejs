<%- include('partials') %>

<div class="" style=" margin-left: 240px; width:auto; padding-top: 30px; padding-left: 20px;">


<form action="/admin/updatecategory/<%= category._id %>" method="POST">
    <div class="form-group">
        <div class="row justify-content-center">
            <div class="col-md-4">
                
                <input type="text" class="form-control" placeholder="" id="category" name="category" value="<%= category.categoryName %>" required>
            </div>
            <div class="col-md-3">
                <button type="submit" class="btn btn-primary">Update Category</button>
            </div>
        </div>
        
    </div>
    
</form>


</div>

